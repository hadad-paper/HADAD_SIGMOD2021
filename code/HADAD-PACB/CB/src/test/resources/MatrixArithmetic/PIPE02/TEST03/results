RW0<idr4> :- constructMatrixValue(i,j,v,idr1),constructMatrixValue(i,x,z,idr2),matrixFilter(idr1,"5",idr1),matrixFilter(idr2,"6",idr2),V1(i,j,v),V2(i,x,z),add(idr1,idr2,idr3),constructName(idr1,"A"),constructName(idr2,"B"),name(idr1,"A"),name(idr2,"B"),tr(idr3,idr4);
RW1<idr4> :- constructMatrixValue(i,j,v,idr1),constructMatrixValue(i,x,z,idr2),matrixFilter(idr1,"5",idr1),V1(i,j,v),V2(i,x,z),add(idr1,idr2,idr3),eq(z,"6"),constructName(idr1,"A"),constructName(idr2,"B"),name(idr1,"A"),name(idr2,"B"),tr(idr3,idr4);
RW2<idr4> :- constructMatrixValue(i,j,v,idr1),constructMatrixValue(i,x,z,idr2),matrixFilter(idr2,"6",idr2),V1(i,j,v),V2(i,x,z),add(idr1,idr2,idr3),eq(v,"5"),constructName(idr1,"A"),constructName(idr2,"B"),name(idr1,"A"),name(idr2,"B"),tr(idr3,idr4);
RW3<idr4> :- constructMatrixValue(i,j,v,idr1),constructMatrixValue(i,x,z,idr2),V1(i,j,v),V2(i,x,z),add(idr1,idr2,idr3),eq(v,"5"),eq(z,"6"),constructName(idr1,"A"),constructName(idr2,"B"),name(idr1,"A"),name(idr2,"B"),tr(idr3,idr4);
