RW0<t> :- netflix(i,j,v),multi(idU,idVT,idr1),multi(idr2,idV1,t),constructMatrixValue(i,j,v,idr3),add(idr1,idr3,idr2),constructName(idr3,"X"),name(idU,"U"),name(idV1,"V1"),name(idr3,"X"),matrixFilterGT(idr3,"4",idr3),tr(idV1,idVT);
