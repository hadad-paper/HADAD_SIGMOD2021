RW2<t> :- netflix(i,j,v),multi(idU,idVT,idr1),multi(idr1,idV1,SK_27),multi(idr3,idV1,SK_28),constructMatrixValue(i,j,v,idr3),add(SK_27,SK_28,t),constructName(idr3,"X"),name(idU,"U"),name(idV1,"V1"),name(idr3,"X"),matrixFilterGT(idr3,"4",idr3),tr(idV1,idVT);
