RW0<d> :- add(idr1,idr2,r),rowSums(r,d),size(idA,"1000000","100"),size(idB,"1000000","100"),name(idA,"A"),name(idB,"B"),tr(idA,idr1),tr(idB,idr2);
RW1<d> :- add(idr2,idr1,r),rowSums(r,d),size(idA,"1000000","100"),size(idB,"1000000","100"),name(idA,"A"),name(idB,"B"),tr(idA,idr1),tr(idB,idr2);
RW2<d> :- add(idr1,idr2,r),colSums(SK_198,SK_199),size(idA,"1000000","100"),size(idB,"1000000","100"),name(idA,"A"),name(idB,"B"),tr(idA,idr1),tr(idB,idr2),tr(SK_198,r),tr(SK_199,d);
RW3<d> :- add(idr2,idr1,r),colSums(SK_198,SK_199),size(idA,"1000000","100"),size(idB,"1000000","100"),name(idA,"A"),name(idB,"B"),tr(idA,idr1),tr(idB,idr2),tr(SK_198,r),tr(SK_199,d);
RW4<d> :- add(idr1,idr2,r),rowSums(r,d),name(idA,"A"),name(idB,"B"),name(SK_198,"V"),tr(idA,idr1),tr(idB,idr2);
RW5<d> :- add(idr2,idr1,r),rowSums(r,d),name(idA,"A"),name(idB,"B"),name(SK_198,"V"),tr(idA,idr1),tr(idB,idr2);
RW6<d> :- add(idA,idB,SK_198),rowSums(r,d),size(idA,"1000000","100"),size(idB,"1000000","100"),name(idA,"A"),name(idB,"B"),tr(SK_198,r);
RW7<d> :- add(idB,idA,SK_198),rowSums(r,d),size(idA,"1000000","100"),size(idB,"1000000","100"),name(idA,"A"),name(idB,"B"),tr(SK_198,r);
RW8<d> :- rowSums(r,d),name(SK_198,"V"),tr(SK_198,r);
RW9<d> :- add(idA,idB,SK_198),colSums(SK_198,SK_199),size(idA,"1000000","100"),size(idB,"1000000","100"),name(idA,"A"),name(idB,"B"),tr(SK_199,d);
RW10<d> :- add(idB,idA,SK_198),colSums(SK_198,SK_199),size(idA,"1000000","100"),size(idB,"1000000","100"),name(idA,"A"),name(idB,"B"),tr(SK_199,d);
RW11<d> :- colSums(SK_198,SK_199),name(SK_198,"V"),tr(SK_199,d);
