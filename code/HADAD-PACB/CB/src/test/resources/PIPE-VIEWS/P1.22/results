RW0<t> :- add(idC,idD,idr1),in(idr1,idr2),trace(idr2,t),size(idD,"10000","10000"),size(idC,"10000","10000"),name(idD,"D"),name(idC,"C");
RW1<t> :- add(idD,idC,idr1),in(idr1,idr2),trace(idr2,t),size(idD,"10000","10000"),size(idC,"10000","10000"),name(idD,"D"),name(idC,"C");
RW2<t> :- trace(idr2,t),name(idr2,"V");
