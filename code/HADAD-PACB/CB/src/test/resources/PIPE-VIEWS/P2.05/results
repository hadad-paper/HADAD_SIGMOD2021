RW0<d> :- add(idC,idD,idr1),in(idr1,idr2),det(idr2,d),size(idD,"10000","10000"),size(idC,"10000","10000"),name(idD,"D"),name(idC,"C");
RW1<d> :- add(idD,idC,idr1),in(idr1,idr2),det(idr2,d),size(idD,"10000","10000"),size(idC,"10000","10000"),name(idD,"D"),name(idC,"C");
RW2<d> :- det(idr2,d),name(idr2,"V");
