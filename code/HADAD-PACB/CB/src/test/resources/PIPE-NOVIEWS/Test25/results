RW0<d> :- add(idA,idB,idr1),sum(idr1,d),size(idA,"1000000","100"),size(idB,"1000000","100"),name(idA,"A"),name(idB,"B");
RW1<d> :- add(idB,idA,idr1),sum(idr1,d),size(idA,"1000000","100"),size(idB,"1000000","100"),name(idA,"A"),name(idB,"B");
RW2<d> :- sum(idA,SK_6696),sum(idB,SK_6697),add_s(SK_6696,SK_6697,d),size(idA,"1000000","100"),size(idB,"1000000","100"),name(idA,"A"),name(idB,"B");
RW3<d> :- sum(idA,SK_6696),sum(idB,SK_6697),add_s(SK_6697,SK_6696,d),size(idA,"1000000","100"),size(idB,"1000000","100"),name(idA,"A"),name(idB,"B");
