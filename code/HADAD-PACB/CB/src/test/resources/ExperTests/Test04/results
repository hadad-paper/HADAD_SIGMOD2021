RW0<idr5> :- multi(idX,idHT,idr1),multi(idW,idH,idr2),multi(idr2,idHT,idr3),multie(idW,idr4,idr5),div(idr1,idr3,idr4),name(idX,"X"),name(idH,"H"),name(idW,"W"),tr(idH,idHT);
RW1<idr5> :- multi(idX,idHT,idr1),multi(idH,idHT,SK_0),multi(idW,SK_0,idr3),multie(idW,idr4,idr5),div(idr1,idr3,idr4),name(idX,"X"),name(idH,"H"),name(idW,"W"),tr(idH,idHT);
