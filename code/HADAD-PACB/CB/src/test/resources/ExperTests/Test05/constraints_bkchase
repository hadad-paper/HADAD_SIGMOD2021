#EGDs
name(id1,n1),name(id2,n1)->id2=id1;
size(idA,n1,n2),size(idA,n3,n4)->n3=n1,n4=n2;
multi(idA,idB,idr1),multi(idA,idB,idr2)->idr2=idr1;
trace(id1,idr1),trace(id2,idr2)->idr2=idr1;
