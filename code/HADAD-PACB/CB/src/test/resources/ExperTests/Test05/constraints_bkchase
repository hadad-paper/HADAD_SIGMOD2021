tr(id1,idr1),tr(id1,idr2)->idr2=idr1;
in(id1,idr1),in(id1,idr2)->idr2=idr1;
multi_e(id1,id2,idr1),multi_e(id1,id2,idr2)->idr2=idr1;



AL(idA,idB,idr2)->tr(idB,idr1),multi_e(idA,idr1,idr3),sum(idr3,idr2);