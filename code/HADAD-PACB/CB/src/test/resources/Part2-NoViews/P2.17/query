#(((M+N)T)-1)*(D-1-1)*(Z-1Z)
Q<idr5>:-
name(idM,"M"),
name(idN,"N"),
name(idD,"D"),
name(idZ,"Z"),
add(idM,idN,idr1),
in(idr1,idr2),
tr(idr2,idr3),
in(idD,idDT1),
in(idDT1,idDT2),
multi(idr3,idDT2,idr4),
in(idZ,idrZ),
multi(idZ,idrZ,idrI),
multi(idr4,idrI,idr5);


