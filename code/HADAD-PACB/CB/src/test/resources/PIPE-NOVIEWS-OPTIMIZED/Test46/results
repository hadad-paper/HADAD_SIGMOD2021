RW0<d> :- add(idD,idC,idr1),in(idr1,i),trace(i,d),size(idD,"10000","10000"),size(idC,"10000","10000"),name(idD,"D"),name(idC,"C");
RW1<d> :- add(idC,idD,idr1),in(idr1,i),trace(i,d),size(idD,"10000","10000"),size(idC,"10000","10000"),name(idD,"D"),name(idC,"C");
RW2<d> :- in(idr1,i),trace(i,d),name(idr1,"V");
