RW0<idr2> :- multi(idM,idN,idr1),multi(idr1,idC,idr2),size(idM,"50000","100"),size(idN,"100","50000"),size(idC,"50000","100"),name(idM,"M"),name(idN,"N"),name(idC,"C");
RW1<idr2> :- multi(idN,idC,SK_0),multi(idM,SK_0,idr2),size(idM,"50000","100"),size(idN,"100","50000"),size(idC,"50000","100"),name(idM,"M"),name(idN,"N"),name(idC,"C");
