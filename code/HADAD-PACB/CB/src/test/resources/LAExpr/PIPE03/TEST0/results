RW3<idr4> :- add(idZ,idr2,idr3),in(idM,SK_0),in(idN,SK_1),multi(SK_1,SK_0,idr2),det(idr3,idr4),name(idM,"M"),name(idN,"N"),name(idZ,"Z");
RW4<idr4> :- add(idr2,idZ,idr3),det(idr3,idr4),name(idZ,"Z"),name(idr2,"V");
RW5<idr4> :- add(idZ,idr2,idr3),det(idr3,idr4),name(idZ,"Z"),name(idr2,"V");
