RW0<idr4> :- add(idr2,idZ,idr3),in(idr1,idr2),multi(idM,idN,idr1),det(idr3,idr4),name(idM,"M"),name(idN,"N"),name(idZ,"Z");
RW1<idr4> :- add(idZ,idr2,idr3),in(idr1,idr2),multi(idM,idN,idr1),det(idr3,idr4),name(idM,"M"),name(idN,"N"),name(idZ,"Z");
