VTEXT= FOR  PJ:{FROM MIMIC AS P, JSONARRAYELEMENTS(P->'ADMISSIONS') AS A, JSONARRAYELEMENTS(A->'noteevents') AS NOTEVEENTS
				SELECT P->'SUBJECT_ID' AS SUBJECT_ID , A->'HADM_ID' AS HADM_ID, NOTEVEENTS->'TEXT' AS TEXT } 
RETURN	 SJ:{ SOLRJSONBUILDOBJECT ("SUBJECT_ID":SUBJECT_ID, "HADM_ID":HADM_ID,"TEXT":TEXT )}