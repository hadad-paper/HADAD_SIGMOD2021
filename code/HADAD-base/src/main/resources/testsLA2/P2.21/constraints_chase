#---------------TRANS---------------------#
tr(idA,idr2),in(idr2,idr3)->in(idA,idr1),tr(idr1,idr3);
#---------------INV-----------------------#
multi(idA,idB,idr1),in(idr1,idr2)->in(idA,idAT),in(idB,idBT),multi(idBT,idAT,idr2);


#--------------MULTI----------------------#
