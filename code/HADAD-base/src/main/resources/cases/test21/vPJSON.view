V21_JSONT= FOR  PJ:{FROM MIMIC AS P, MIMIC_D AS D, JSONARRAYELEMENTS(P->'ADMISSIONS') AS A, JSONARRAYELEMENTS(A->'labevents') AS LABEVENTS, JSONARRAYELEMENTS(A->'ICUSTAYS') AS ICUSTAYS, JSONARRAYELEMENTS(ICUSTAYS->'PRESCRIPTIONS') AS PRESCRIPTIONS  
			WHERE D->'ITEMID'=LABEVENTS->'ITEMID' AND
				  D->'FLUID'= "Cerebrospinal Fluid (CSF)" AND
				  PRESCRIPTIONS->'DRUG_TYPE'="MAIN" AND
				  LABEVENTS->'FLAG'="abnormal" 
				  
			SELECT P->'SUBJECT_ID' AS SUBJECT_ID , A->'HADM_ID' AS HADM_ID } 
RETURN	 PJ:{ JSONBUILDOBJECT ('SUBJECT_ID',SUBJECT_ID, 'HADM_ID',HADM_ID) }