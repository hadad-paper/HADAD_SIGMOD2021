V12_JSONT= FOR  PJ:{FROM MIMIC AS P, MIMIC_D AS D, JSONARRAYELEMENTS(P->'ADMISSIONS') AS A, JSONARRAYELEMENTS(A->'labevents') AS LABEVENTS
			WHERE D->'ITEMID'=LABEVENTS->'ITEMID' AND
				  D->'FLUID'= "Cerebrospinal Fluid (CSF)" AND
				  LABEVENTS->'FLAG'="abnormal"
				  
			SELECT P->'SUBJECT_ID' AS SUBJECT_ID , A->'HADM_ID' AS HADM_ID, A->'MICROBIOLOGYEVENTS' AS MICROBIOLOGYEVENTS, A->'PROCEDUREEVENTS_MV' AS PROCEDUREEVENTS_MV } 
RETURN	 PJ:{ JSONBUILDOBJECT ('SUBJECT_ID',SUBJECT_ID, 'HADM_ID',HADM_ID, 'MICROBIOLOGYEVENTS',MICROBIOLOGYEVENTS, 'PROCEDUREEVENTS_MV',PROCEDUREEVENTS_MV ) }